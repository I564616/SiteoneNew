## messageSource=classpath:/siteonecore/messages/email-siteOneJobsStatusEmail_$lang.properties

<html>
	<head>
	</head>
	<body bgcolor="#ffffff">
      <table width="80%" cellpadding="0" cellspacing="0" border="1"> 
		 <tr bgcolor="#e0e0e0">
			<td width="27%" height="30" align="left">
			  <font color="#000000" >&nbsp;JOB NAME</font>
		    </td>
			<td width="27%" height="30" align="left">
			  <font color="#000000" >LAST START TIME</font>
		    </td>
			<td width="27%" height="30" align="left">
			  <font color="#000000" >LAST END TIME</font>
		    </td>
			<td width="19%" height="30" align="left" style="padding-right:4%;">
			  <font color="#000000" >LAST STATUS
			   <b/>
			  </font>
			</td>
		</tr>
        #foreach( $job in ${ctx.jobsList} )
	     <tr>
			<td valign="top" width="27%" align="left" style="font-color:#000000"> 
			  #if($job.code!='null')
			    $job.code
			  #else
			  #end  
			</td>
			<td valign="top" width="27%" align="left" style="font-color:#000000;">
			  #if($job.startTime!='null')
			    $job.startTime
			  #else
			  #end 
			 </td>
            <td valign="top" width="27%" align="left" style="font-color:#000000;">
              #if($job.endTime!='null')
			    $job.endTime
			  #else
			  #end 
            </td>
            <td valign="top" width="19%" align="left" style="font-color:#000000;">
               #if($job.status!='null')
			    $job.status
			  #else
			  #end 
            </td>
		 </tr>
        #end
	</table>
 </body>
</html>