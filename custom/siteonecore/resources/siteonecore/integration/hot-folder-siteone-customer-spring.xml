		p:mapping="siteonecustomer"
		p:converter-ref="batchSiteOneDefaultBillingAddressUnitConverter" />

	<bean id="batchSiteOneDefaultBillingAddressUnitConverter" class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexConverter">
		<property name="header">
			<value>
				UPDATE B2Bunit;uid[unique=true];billingAddress(owner(B2BUnit.uid),guid,billingAddress)
			</value>
		</property>
		<property name="impexRow">
			<value>;{+5}_{4};{5}_{4}:{+2}:TRUE</value>
		</property>
		<property name="rowFilter">
			<bean class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexRowFilter">
				<property name="expression" value="row[3]=='billing_address'"/>
			</bean>
		</property>
		<property name="type" value="B2BUnit"/>
	</bean>

	<!-- Set Default Billing Address Ends -->

	<!-- Create dummy contact for every account which will be mapped in order for store contact -->

	<bean id="batchSiteOneStoreContactConverterMapping"
		class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.mapping.impl.DefaultConverterMapping"
		p:mapping="siteonecustomer"
		p:converter-ref="batchSiteOneStoreContactConverter" />

	<bean id="batchSiteOneStoreContactConverter" class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexConverter">
		<property name="header">
			<value>
				INSERT_UPDATE B2BCustomer;uid[unique=true][cellDecorator=com.siteone.core.batch.decorator.UserIdCellDecorator];email;defaultB2BUnit(uid);name;payBillOnline[default=false];loginDisabled[default=false]
			</value>
		</property>
		<property name="impexRow">
			<value>;storecontact-{0}_{4};storecontact@{0}_{4}.com;{0}_{4};storecontact-{0}_{4};{30};0</value>
		</property>
		<property name="rowFilter">
			<bean class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexRowFilter">
				<property name="expression" value="row[3]=='primary_account' || row[3]=='shipto_account'"/>
			</bean>
		</property>
	</bean>

</beans>
