	      class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.mapping.impl.DefaultConverterMapping" 
	      p:mapping="somedia" p:converter-ref="batchSOPhysicalImageError"/> 
	
	<bean id="batchSOPhysicalImageError" class="com.siteone.pcm.exception.PcmImageErrorHandler">
		 <property name="header">
			<value>
			#error
			</value>
		</property> 
		<property name="impexRow">
			<value>#-{+0},{1};{2};{3};{+4};{5};{6}</value>
		</property>
		<property name="blobDataImportService" ref="blobDataImportService"/>
		<property name="configurationService" ref="configurationService"/>
	</bean>
	
	<bean id="batchSOPhysicalImageLoaderMapping" 
	      class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.mapping.impl.DefaultConverterMapping" 
	      p:mapping="somedia" p:converter-ref="batchSOPhysicalImageLoader"/> 
	
	<bean id="batchSOPhysicalImageLoader" class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexConverter">
		<property name="header">
			<value>	#{defaultImpexProductHeader}
					# Import media from filesystem
					$productCatalog=siteoneProductCatalog
					$catalogVersion=catalogversion(catalog(id[default=$productCatalog]),version[default='Staged'])[unique=true,default=$productCatalog:Staged]
					$importFolder=file:$BASE_SOURCE_DIR$/media
					INSERT_UPDATE Media;code[unique=true];mime[default='image/jpeg'];$catalogVersion;folder(qualifier)[default=root];realfilename;@media[translator=de.hybris.platform.impex.jalo.media.MediaDataTranslator];sds;label;
			</value>
		</property>
		<property name="impexRow">
			<value>;{+0}_{+3};{+4};;;{+1};$importFolder/{+1};{5};{6};</value>
		</property>
		<property name="rowFilter">
			<bean class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexRowFilter">
				<property name="expression" value="row[0]!=''  &amp;&amp; row[1]!=''  &amp;&amp; row[3]!=''  &amp;&amp; row[2]==''   &amp;&amp; row[5]=='' &amp;&amp; row[6] == ''  &amp;&amp; (row[4]=='image/jpeg' || row[4]=='image/jpg')"/>
			</bean>
		</property>
		<property name="type" value="Media"/>
	</bean>

