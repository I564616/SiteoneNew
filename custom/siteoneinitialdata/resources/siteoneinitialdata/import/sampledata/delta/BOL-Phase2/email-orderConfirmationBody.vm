									#if (${ctx.discountedValue}  != '0.00')
									<tr>
										<td>&nbsp;</td>
										<td>&nbsp;</td>
										<td>&nbsp;</td>
										<td class="paymentStyle2">${ctx.messages.totalDiscounts}</td>
										<td colspan="2" align="right" style="color:red;padding-left:0px; padding-right:25px;">-${ctx.order.totalDiscounts.formattedValue}</td>
									</tr>
									#end
									<tr>
										<td>&nbsp;</td>
										<td>&nbsp;</td>
										<td>&nbsp;</td>
										<td class="paymentStyle">${ctx.messages.tax}</td>
										<td colspan="2" align="right" style="padding-right:25px;">${ctx.order.totalTax.formattedValue}</td>
									</tr>
									<tr>
										<td>&nbsp;</td>
										<td>&nbsp;</td>
										<td>&nbsp;</td>
										<td class="paymentStyle1">${ctx.messages.total}</td>
										<td colspan="2" align="right" style="padding-right:25px; font-weight: bold;">${ctx.order.totalPriceWithTax.formattedValue}</td>
									</tr>
									<tr>
										<td colspan="6" style="padding:10px 20px;">
											<table width="100%">
												<tr>
													<td style="background-color: #cccccc;padding-left: 5px;text-transform: uppercase; height: 30px;">${ctx.messages.payment}</td>
												</tr>
											</table>
										</td>
									</tr>
									#if(${ctx.paymentType} == "2")
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"><b>${ctx.messages.payatbranch}</b></td>
										</tr>
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"> <b> ${ctx.messages.branch}: </b> ${ctx.order.pointOfService.address.town}  #${ctx.order.pointOfService.storeId}</td>
										</tr>
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"> <b> ${ctx.messages.amount}:</b> ${ctx.order.totalPriceWithTax.formattedValue} </td>
										</tr>
									#elseif(${ctx.paymentType} == "3")
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"><b>${ctx.messages.creditcard}</b></td>
										</tr>
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"> <b> ${ctx.messages.cardNumber}: </b>${ctx.cardNumber} </td>
										</tr>
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"> <b> ${ctx.messages.amount}:</b> ${ctx.order.totalPriceWithTax.formattedValue} </td>
										</tr>
									#else
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"><b>${ctx.messages.payatbranch}</b></td>
										</tr>
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"> <b> ${ctx.messages.branch}: </b> ${ctx.order.pointOfService.address.town}  #${ctx.order.pointOfService.storeId}</td>
										</tr>
										<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;"> <b> ${ctx.messages.amount}:</b> ${ctx.order.totalPriceWithTax.formattedValue} </td>
										</tr>
									#end
									<tr>
										<td colspan="6" style="padding:10px 20px;">
											<table width="100%">
												<tr>
													<td style="background-color: #cccccc;padding-left: 5px;text-transform: uppercase; height: 30px;">${ctx.messages.billinhInfo}</td>
												</tr>
											</table>
										</td>
									</tr>
									<tr>
										<td colspan="6" style="padding: 10px 20px 0px 30px;">${ctx.messages.billingMsg}</td>
									</tr>
									<tr>
										<td>&nbsp;</td>
									</tr>
									<tr>
										<td colspan="6" style="padding:0 30px;">${ctx.messages.billingsubmsg},
											<a href="${ctx.baseThemeUrl}/store/${ctx.order.pointOfService.storeId}" style="color:#50a0c5;"> ${ctx.order.pointOfService.address.town} ${ctx.order.pointOfService.storeId}</a> at
											<a href="tel:${ctx.order.pointOfService.address.phone}" style="color:#50a0c5;">${ctx.order.pointOfService.address.phone}</a>.
										</td>
									</tr>
									<tr>
										#set ($contactUs = "${ctx.baseUrl}/faq")
										#set ( $contactUsMsg = ${ctx.messages.getMessage('contactUsMsg', "#genHtmlLinkStartTag($contactUs)", "#genHtmlLinkEndTag()")} )
										<td colspan="6" style="padding: 20px 20px 20px 30px;">
											<span style="text-decoration:none; color:#000000">${ctx.messages.contactmsg}</span>
											<a href="${ctx.baseUrl}/contactus" style="color:#50a0c5">${ctx.messages.contactyou}</a>.
										</td>
									</tr>
									#if(${ctx.order.orderType} == "DELIVERY")
									<tr>
										<td colspan="6" style="padding: 20px 20px 20px 30px;">
										${ctx.messages.deliveryMessage}<br><br>${ctx.messages.orderMsgs}<br><br>${ctx.messages.regulatoryMsg}
										</td>
									</tr>
									#end
									<tr>
										<td colspan="6" style="padding:10px 30px;">${ctx.messages.complimentaryClose}
											<br>
											<span style="font-weight: bold; font-style: italic;">${ctx.messages.signaturename}</span>
										</td>
									</tr>
									<tr>
										<td>&nbsp;</td>
									</tr>
								</table>
							</td>
						</tr>
					</table>
				</td>
			</tr>
		</table>
	</body>
</html>
